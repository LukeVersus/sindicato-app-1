<!DOCTYPE html>
<html lang="pt-br">

<head>

	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<meta name="description" content="">
	<meta name="author" content="">

	<title><%- process.env.TITLE %></title>

	<!-- Custom fonts for this template-->
	<link href="/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
	<link
		href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
		rel="stylesheet">

	<!-- Custom styles for this template-->
	<link href="/css/sb-admin-2.min.css" rel="stylesheet">

	<!-- Custom styles for this page -->
	<link href="/vendor/datatables/dataTables.bootstrap4.min.css" rel="stylesheet">
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js" type="text/javascript"></script>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.mask/1.14.11/jquery.mask.min.js"></script>

	<script>
		function myFunction() {
			var modalidade = $('#modalidade').val();
			if (modalidade == 'PRESENCIAL') {
				var selectCourse = document.getElementById('moodle');
				selectCourse.style.visibility = 'hidden';
				var precisadef = document.getElementById('precisadefer');
				precisadef.style.visibility = 'hidden';
				$('#course').val('');
			} else {
				$('#course').prop('disabled', false);
				$('#course').prop('required', false);
				$('#precisadef').prop('disabled', false);
				var selectCourse = document.getElementById('moodle');
				selectCourse.style.visibility = 'visible';
				var precisadef = document.getElementById('precisadefer');
				precisadef.style.visibility = 'visible';
			}
		}

		$('#cpf').mask('000.000.000-00');
		$('#tel_res').mask('(00) 0000-0000');
		$('#tel_cel').mask('(00) 00000-0000');
		$('#tel_trab').mask('(00) 0000-0000');
		$('#cep').mask('00000-000');
	</script>

</head>

<body id="page-top">

	<!-- Page Wrapper -->
	<div id="wrapper">

		<!-- Menu -->
		<%- include ../template/menu.ejs %>

		<!-- Content Wrapper -->
		<div id="content-wrapper" class="d-flex flex-column">

			<!-- Main Content -->
			<div id="content">

				<!-- Header -->
				<%- include ../template/topo.ejs %>

				<!-- Begin Page Content -->
				<div class="container-fluid">

					<!-- Page Heading -->
					<h1 class="h3 mb-4 text-gray-800">Cadastrar Associado</h1>

					<div class="row">

						<!-- Earnings (Monthly) Card Example -->
						<div class="col-xl-12 col-md-12 mb-4">
							<div class="card border-left-primary shadow h-100 py-2">
								<div class="card-body">
									<div class="row no-gutters align-items-center">
										<div class="col mr-2">
											<form class="user" method="POST" action="/app/<%= page %>/create/submit">

												<fieldset
													style="border: 1px groove #FFF !important; padding: 0 1.4em 1.4em 1.4em !important; margin: 0 0 1.5em 0 !important;">
													<legend style="width: auto; padding:0 10px; border-bottom:none;">
														Informações Gerais</legend>

													<div class="form-group row">
														<div class="col-sm-6">
															Nome:
															<input type="text" class="form-control" id="nome"
																name="nome" required>
														</div>

														<div class="col-sm-3">
															Matrícula:
															<input type="text" class="form-control" id="matricula"
																name="matricula" required>
														</div>

														<div class="col-sm-3">
															Nível:
															<select name="nivel" id="nivel" class="form-control">
																<option value="SERVIDOR">Servidor Público Estadual
																</option>
																<option value="NAO_SERVIDOR">Outro</option>
															</select>
														</div>
													</div>

													<div class="form-group row">
														<div class="col-sm-3">
															CPF:
															<input type="text" class="form-control" id="cpf" name="cpf"
																required>
														</div>

														<div class="col-sm-3">
															RG:
															<input type="text" class="form-control" id="rg" name="rg"
																required>
														</div>

														<div class="col-sm-3">
															Órgão Expedidor:
															<input type="text" class="form-control" id="orgao_expedidor"
																name="orgao_expedidor" required>
														</div>

														<div class="col-sm-3">
															Sexo:
															<select name="sexo" id="sexo" class="form-control">
																<option value="MASCULINO">MASCULINO
																</option>
																<option value="FEMININO">FEMININO</option>
															</select>
														</div>

													</div>
													<div class="form-group row">

														<div class="col-sm-3">
															Estado Civil:
															<select name="tpEstadoCivil" id="tpEstadoCivil"
																class="form-control">
																<option value="SOLTEIRO">SOLTEIRO(A)
																</option>
																<option value="CASADO">CASADO(A)</option>
																<option value="UNIAO_ESTAVEL">UNIAO_ESTAVEL</option>
																<option value="DIVORCIADO">DIVORCIADO(A)</option>
																<option value="VIUVO">VIUVO(A)</option>
																<option value="SEPARADO">SEPARADO(A)</option>
															</select>
														</div>

														<div class="col-sm-3">
															Data de nascimento: *
															<input type="date" class="form-control" id="datanascimento"
																name="datanascimento" required="">
														</div>

														<div class="col-sm-3">
															Telefone Residencial:
															<input type="text" class="form-control" id="tel_res"
																name="tel_res" maxlength="15">
														</div>

														<div class="col-sm-3">
															Telefone Celular:
															<input type="text" class="form-control" id="tel_cel"
																name="tel_cel" maxlength="15">
														</div>

													</div>

												</fieldset>

												<fieldset
													style="border: 1px groove #FFF !important; padding: 0 1.4em 1.4em 1.4em !important; margin: 0 0 1.5em 0 !important;">

													<legend style="width: auto; padding:0; border-bottom:none;">Endereço
													</legend>

													<div class="form-group row">
														<div class="col-sm-6">
															Logradouro:
															<input type="text" class="form-control" id="logradouro"
																name="logradouro" required>
														</div>
														<div class="col-sm-2">
															Número:
															<input type="text" class="form-control" id="numero"
																name="numero" required>
														</div>
														<div class="col-sm-4">
															Complemento:
															<input type="text" class="form-control" id="complemento"
																name="complemento">
														</div>
													</div>

													<div class="form-group row">
														<div class="col-sm-5">
															Bairro:
															<input type="text" class="form-control" id="bairro"
																name="bairro" required>
														</div>
														<div class="col-sm-3">
															CEP:
															<input type="text" class="form-control" id="cep" name="cep"
																required>
														</div>

														<div class="col-sm-4">
															Município:
															<select class="form-control" id="municipio"
																name="municipio">
																<% for (var i = 0; i < itensEstados.length; i++) { %>
																<% if (itensEstados[i].id != null ) { %>
																<option id="estadoid" value="<%- itensEstados[i].id %>">
																	<%- itensEstados[i].nome %>
																</option>
																<% } %>
																<% } %>
															</select>
														</div>

													</div>

												</fieldset>

												<fieldset
													style="border: 1px groove #FFF !important; padding: 0 1.4em 1.4em 1.4em !important; margin: 0 0 1.5em 0 !important;">

													<legend style="width: auto; padding:0; border-bottom:none;">Dados
														Profissionais
													</legend>

													<div class="form-group row">

														<div class="col-sm-3">
															Município de Lotação:
															<select class="form-control" id="municipio_lotacao"
																name="municipio_lotacao">
																<% for (var i = 0; i < itensEstados.length; i++) { %>
																<% if (itensEstados[i].id != null ) { %>
																<option id="estadoid" value="<%- itensEstados[i].id %>">
																	<%- itensEstados[i].nome %>
																</option>
																<% } %>
																<% } %>
															</select>
														</div>

														<div class="col-sm-6">
															Escola de Trabalho:
															<input type="text" class="form-control" id="nome_escola"
																name="nome_escola" required>
														</div>

														<div class="col-sm-3">
															Turno:
